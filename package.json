{
  "name": "the-hague-maps",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "leaflet": "^1.3.1",
    "leaflet.markercluster": "~1.3.0",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "object-path": "^0.11.4",
    "prop-types": "^15.6.1",
    "query-string": "^5.1.1",
    "react": "^16.3.0-alpha.3",
    "react-day-picker": "^7.1.4",
    "react-dom": "^16.3.0-alpha.3",
    "react-leaflet": "^1.9.1",
    "react-leaflet-markercluster": "~1.1.8",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-table": "^6.8.0",
    "react-transition-group": "^2.2.1",
    "recharts": "^1.0.0-beta.10"
  },
  "scripts": {
    "lint": "eslint --ext .js --ext .js src/**",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch true --recursive true --source-map-contents sass --source-map-embed true",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/object-path": "^0.9.29",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.0.40",
    "@types/react-router-dom": "^4.2.5",
    "@types/react-table": "^6.7.7",
    "@types/react-transition-group": "^2.0.7",
    "@types/recharts": "^1.0.18",
    "babel-eslint": "7",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-scripts": "^1.1.1",
    "source-map-explorer": "^1.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier-eslint",
      "git add"
    ]
  }
}
